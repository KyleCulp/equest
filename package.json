{
  "name": "equest",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lerna": "node_modules/.bin/lerna",
    "syncpack": "syncpack",
    "clean": "npm run lerna clean",
    "bootstrap": "npm run lerna bootstrap",
    "link": "npm run lerna link",
    "build": "npm run lerna run build",
    "init": "node -r dotenv/config scripts/init.js",
    "server:dev": "npm run dev --prefix @equest/server",
    "database:reset": "npm run reset --prefix @equest/database",
    "database:watch": "npm run watch --prefix @equest/database",
    "database:commit": "npm run commit --prefix @equest/database",
    "database:uncommit": "npm run uncommit --prefix @equest/database",
    "database:migrate": "npm run migrate --prefix @equest/database",
    "graphql:codegen": "npm run codegen --prefix @equest/graphql",
    "graphql:watch": "npm run watch --prefix @equest/graphql",
    "client:dev": "npm run dev --prefix @equest/client"
  },
  "dependencies": {
    "pg": "^7.18.2",
    "lerna": "^3.20.2",
    "dotenv": "^8.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "npm": "^6.14.4",
    "prompt": "^1.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "syncpack": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleculp/equest.git"
  }
}
