{
  "name": "equest-backend",
  "version": "0.0.1",
  "description": "Backend API of Equest Pro",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start:prod": "npm run build && node dist/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix --quiet",
    "gm": "node -r dotenv/config ./node_modules/.bin/graphile-migrate",
    "tsc": "./node_modules/.bin/tsc",
    "db:init": "node -r dotenv/config scripts/createDBOwner.js",
    "db:migrate": "npm run gm migrate",
    "db:watch": "npm run gm watch",
    "db:commit": "npm run gm commit",
    "db:uncommit": "npm run gm uncommit",
    "db:indexCheck": "node -r dotenv/config scripts/check-indexes.js",
    "db:reset": "npm run gm reset",
    "dump-schema": "pg_dump -s -v -f data/schema.sql postgres://postgres@postgres/dev --no-sync --schema-only --no-owner --exclude-schema=graphile_migrate --exclude-schema=graphile_worker"
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@graphile-contrib/pg-many-to-many": "^1.0.0-beta.12",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphile-migrate": "0.0.17",
    "graphile-worker": "^0.2.0",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.0",
    "pg": "^7.17.0",
    "postgraphile": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/connect-redis": "0.0.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/http-proxy": "^1.17.2",
    "@types/ioredis": "^4.0.20",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.1",
    "@types/passport": "^1.0.2",
    "@types/pg": "^7.11.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-tsdoc": "^0.2.1",
    "eslint-plugin-unused-imports": "^0.1.0",
    "http-proxy": "^1.18.0",
    "nodemon": "^2.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typedoc": "^0.16.9"
  }
}
