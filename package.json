{
  "name": "equest",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "node_modules/.bin/jest --passWithNoTests",
    "lerna": "node_modules/.bin/lerna",
    "syncpack": "node_modules/.bin/yncpack",
    "clean": "npm run lerna clean",
    "bootstrap": "npm run lerna bootstrap",
    "lint:css": "node_modules/.bin/stylelint '@equest/**/*.tsx'",
    "lint": "npm run prettier:check && eslint --ext .ts,.tsx,.gql .",
    "lint:fix": "node_modules/.bin/eslint --ext .ts,.tsx,.graphql . --fix; prettier --ignore-path .eslintignore --write '**/*.{ts,tsx,gql,md}'",
    "prettier:check": "node_modules/.bin/prettier --ignore-path .eslintignore --check '**/*.{ts,tsx,gql,md}'",
    "link": "npm run lerna link",
    "build": "npm run lerna run build",
    "init": "node -r dotenv/config scripts/init.js",
    "server:dev": "npm run dev --prefix @equest/server",
    "server:build": "npm run build --prefix @equest/server",
    "database:reset": "npm run reset --prefix @equest/database",
    "database:watch": "npm run watch --prefix @equest/database",
    "database:commit": "npm run commit --prefix @equest/database",
    "database:uncommit": "npm run uncommit --prefix @equest/database",
    "database:migrate": "npm run migrate --prefix @equest/database",
    "graphql:codegen": "npm run codegen --prefix @equest/graphql",
    "graphql:watch": "npm run watch --prefix @equest/graphql",
    "client:dev": "npm run dev --prefix @equest/client",
    "client:storybook": "npm run storybook --prefix @equest/client"
  },
  "dependencies": {
    "pg": "^7.18.2",
    "lerna": "^3.20.2",
    "dotenv": "^8.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-tsdoc": "^0.2.4",
    "jest": "^25.5.4",
    "npm": "^6.14.5",
    "prettier": "^2.0.5",
    "prompt": "^1.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "stylelint": "^13.3.3",
    "stylelint-8-point-grid": "^0.2.6",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "syncpack": "^5.0.1",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleculp/equest.git"
  }
}
