    "database:migrate": "npm run migrate --prefix @equest/database"
  },
  "devDependencies": {
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "lerna": "^3.20.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "pg": "^7.18.2"
  }
