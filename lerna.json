{
  "packages": ["@equest/*"],
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "publish": {
      "message": "chore(release): publish",
      "include": "@equest",
      "registry": "https://verdaccio.pcpb.us"
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": ["--no-package-lock"]
    }
  }
}
