{
  "name": "@equest/frontend",
  "version": "1.0.0",
  "description": "React frontend with Apollo Client",
  "private": true,
  "scripts": {
    "next": "./node_modules/.bin/next",
    "export": "yarn run next build && yarn run next export",
    "dev": "yarn run next -p 4000",
    "start": "node_modules/.bin/next start",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "storybook": "node_modules/.bin/start-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/link-retry": "^2.0.0-beta.3",
    "@blueprintjs/core": "^3.26.1",
    "@emotion/core": "^10.0.28",
    "@equest/client": "^1.0.0",
    "@equest/config": "^1.0.0",
    "@equest/graphql": "^1.0.0",
    "@equest/utils": "^1.0.0",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "^4.0.5",
    "@umijs/hooks": "^1.9.2",
    "emotion-theming": "^10.0.27",
    "graphql": "^15.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-plugin-custom-babel-config": "^1.0.2",
    "next-transpile-modules": "^3.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.6.3",
    "rebass": "^4.0.7"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/rebass": "^4.0.5",
    "@types/rebass__forms": "^4.0.2",
    "babel-plugin-import": "^1.13.0",
    "styled-components": "^5.1.0"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  }
}
