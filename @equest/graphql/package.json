{
  "name": "@equest/graphql",
  "version": "0.2.0",
  "description": "Provides packages access to types & apoll client functions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "codegen": "node_modules/.bin/graphql-code-generator --config codegen.yml",
    "watch": "node_modules/.bin/graphql-code-generator --config dev.codegen.yml --watch",
    "build": "npm run codegen && ../../node_modules/.bin/tsc -b",
    "devbuild": "node_modules/.bin/graphql-code-generator --config dev.codegen.yml && ../../node_modules/.bin/tsc -b"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.43",
    "@apollo/link-retry": "^2.0.0-beta.3",
    "@equest/config": "^0.2.0",
    "@equest/utils": "^0.1.0",
    "apollo-utilities": "^1.3.3",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "i": "^0.3.6",
    "js-cookie": "^2.2.1",
    "npm": "^6.14.4",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.13.1",
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@graphql-codegen/typescript-react-apollo": "^1.13.1",
    "@graphql-codegen/typescript-resolvers": "^1.13.1",
    "@types/dotenv": "^8.2.0",
    "@types/js-cookie": "^2.2.5",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.32",
    "tslib": "^1.11.1"
  }
}
